<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite use-query-string="true">
	<rule>
		<from>/apis/structural-resources/latest/</from>
		<to type="redirect">/structural-resources/v1.0/</to>
	</rule>
	<rule>
		<from>/apis/structural-resources/latest</from>
		<to type="redirect">/structural-resources/v1.0/</to>
	</rule>
	<rule>
		<from>^/apis/structural-resources/v1.0$</from>
		<to type="redirect">/structural-resources/v1.0/</to>
	</rule>
	<rule>
		<from>/apis/structural-resources/swagger-ui/(.*)$</from>
		<to type="forward">/swagger-ui/$1</to>
	</rule>

	<rule>
		<from>/apis/soap/latest/structuralResources</from>
		<to type="redirect">%{context-path}/apis/soap/v1.0/structuralResources</to>
	</rule>
</urlrewrite>